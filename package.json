{
  "name": "stats",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon dist/server.js",
    "build": "tsc -w",
    "prisma:migrateGDB": "prisma migrate dev --schema=./prisma/schema.global.prisma --name init",
    "prisma:migrateEDB": "npx prisma migrate dev --schema=./prisma/schema.entreprise.prisma --name init",
    "prisma:generateGBD": "prisma generate --schema=prisma/schema.global.prisma",
    "prisma:generateEBD": "prisma generate --schema=prisma/schema.entreprise.prisma",
    "prisma:formatGBD": "npx prisma format --schema=prisma/schema.global.prisma",
    "prisma:formatEBD": "npx prisma format --schema=prisma/schema.entreprise.prisma",
    "prisma:GDBseed": "npx ts-node dist/seeders/globalSeeds.js",
    "prisma:EDBseed": "npx ts-node dist/seeders/entrepriseSeeds.js",
    "start": "node dist/index.js"
  },
